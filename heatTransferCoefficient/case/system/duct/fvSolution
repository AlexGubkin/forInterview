FoamFile
{
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    "(rho|rhoFinal)"
    {
        solver          diagonal;
    }

    p_rgh
    {
        solver           GAMG;
        smoother         GaussSeidel;

        tolerance        1e-8;
        relTol           0.01;

        maxIter          100;
    }

    "(U|e|k|epsilon|omega)"
    {
        solver          PBiCGStab;
        preconditioner  DILU;

        tolerance       1e-8;
        relTol          0;
    }

    p_rghFinal
    {
        $p_rgh;
        relTol         0;
    }

    "(U|e|k|epsilon|omega)Final"
    {
        $U;
        relTol          0;
    }
}

PIMPLE
{
    nCorrectors     1;
    nNonOrthogonalCorrectors 2;
    pRefCell        0;
    pRefValue       0;
}

relaxationFactors
{
    equations
    {
        ".*"    0.5;
    }
    fields
    {
        p_rgh   0.1;
        p       0.1;
    }
}

// ************************************************************************* //
