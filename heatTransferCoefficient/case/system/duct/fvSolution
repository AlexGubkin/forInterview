FoamFile
{
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    "(rho|rhoFinal)"
    {
        solver          diagonal;
    }

    p_rgh
    {
        solver           GAMG;
        smoother         GaussSeidel;

        tolerance        1e-8;
        relTol           0.01;

        maxIter          100;
    }

    "(U|h|e|k|epsilon|omega)"
    {
        solver          PBiCGStab;
        preconditioner  DILU;

        tolerance       1e-8;
        relTol          0;
    }

    p_rghFinal
    {
        $p_rgh;
        relTol         0;
    }

    "(U|h|e|k|epsilon|omega)Final"
    {
        $U;
        relTol          0;
    }
}

PIMPLE
{
    nCorrectors     1;
    nNonOrthogonalCorrectors 2;
//     pRefCell        0;
//     pRefValue       0;
}

relaxationFactors
{
    fields
    {
//         rho         1;
//         p_rgh       0.7;
    }

    equations
    {
//         U           0.3;
//         "(h|e)"     0.3;
//         k           0.3;
//         epsilon     0.3;
    }
}

