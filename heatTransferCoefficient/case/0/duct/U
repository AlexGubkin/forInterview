// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

Uinlet          (0 0 20);

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
//         type            fixedValue;
//         value           uniform $Uinlet;
        type            flowRateInletVelocity;
        meanVelocity    20;
//         meanVelocity    table
//         (
//             (   0   0.0)
//             (1000  20.0)
//         );
        profile         turbulentBL;
    }

    outlet
    {
        type            pressureInletOutletVelocity;
        value           $internalField;
    }

    duct_to_pipe
    {
        type            noSlip;
    }

    #includeEtc "caseDicts/setConstraintTypes"
}


