FoamFile
{
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 423.15;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 423.15;
    }

    outlet
    {
//         type            zeroGradient;
        type            inletOutlet;
        inletValue      uniform 723.15;
        value           uniform 723.15;
    }

    duct_to_pipe
    {
        type            coupledTemperature;
        value           $internalField;
    }

    #includeEtc "caseDicts/setConstraintTypes"
}


