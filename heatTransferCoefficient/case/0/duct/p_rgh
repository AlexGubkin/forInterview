FoamFile
{
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 4e6;

boundaryField
{
    inlet
    {
        type            fixedFluxPressure;
        value           $internalField;
    }

    outlet
    {
//         type            fixedValue;
//         value           $internalField;
        type            totalPressure;
        p0              uniform 4e6;
    }

    duct_to_pipe
    {
        type            fixedFluxPressure;
        value           $internalField;
    }

    #includeEtc "caseDicts/setConstraintTypes"
}


